{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": true,
    "moduleResolution": "bundler",
    /* files are emitted by babel */
    "baseUrl": "../",
    "noEmit": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "incremental": true
  },
  "exclude": ["docs/.next", "docs/export", "node_modules"]
}
